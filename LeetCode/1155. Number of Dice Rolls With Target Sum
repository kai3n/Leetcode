class Solution(object):
    def numRollsToTarget(self, d, f, target):
        """
        :type d: int
        :type f: int
        :type target: int
        :rtype: int
        """
        self.res = 0
        def helper(d, f, nums, target):
            if d < 0:
                return
            if d == 0 and sum(nums) == target:
                self.res += 1
            for n in range(1, f+1):
                helper(d-1, f, nums + [n], target)
        for n in range(1, f+1):
            helper(d-1, f, [n], target)
        return self.res
