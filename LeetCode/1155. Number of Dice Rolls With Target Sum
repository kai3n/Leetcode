class Solution(object):
    def numRollsToTarget(self, d, f, target):
        """
        :type d: int
        :type f: int
        :type target: int
        :rtype: int
        """
        def helper(d, f, nums, target):
            if d < 0:
                return 0
            if d == 0 and sum(nums) == target:
                return 1
            return sum(helper(d-1, f, nums + [n], target) for n in range(1, f+1))
        return helper(d, f, [], target)
